{
  "expo": {
    "name": "HoUnits",
    "slug": "HoUnits",
    "privacy": "public",
    "version": "1.2.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#eee"
    },
    "updates": {
      "fallbackToCacheTimeout": 10000
    },
    "assetBundlePatterns": [
      "assets/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [],
      "package": "com.passionfruit.HoUnits",
      "googleServicesFile": "./google-services.json"
    }
  }
}
